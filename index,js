const dataAtual = new Date();

//isso é um comentário de linha

/*
    Isso é um comentário de bloco 
    td é comentário
*/

function CalculaIdade(nascimento)
{
    return dataAtual.getFullYear() - nascimento;
}



console.log(CalculaIdade(1981));
